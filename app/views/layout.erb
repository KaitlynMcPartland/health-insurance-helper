          <li role="presentation"><a href="/users/<%= session[:id] %>/edit">Risk Detector</a></li>
          <li role="presentation"><a href="/users/<%= session[:id] %>">Profile</a></li>
          <li role="presentation"><a href='/sessions/logout'>Log Out</a></li>
