          <li role="presentation"><a href='/sessions/new'>Log In</a></li>
          <li role="presentation"><a href='/users/new'>Sign Up</a></li>

          <% end %>



        </ul>
      </div><!-- /.navbar-collapse -->
    </div><!-- /.container-fluid -->
